<html>
<head>
  <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">
  <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
  <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js"></script>
  <link href="css/bootstrap.min.css" rel="stylesheet">
  <link href="css/font-awesome.min.css" rel="stylesheet">
  <link href="css/prettyPhoto.css" rel="stylesheet">
  <link href="css/price-range.css" rel="stylesheet">
  <link href="css/animate.css" rel="stylesheet">
  <link href="css/main.css" rel="stylesheet">
  <link href="css/responsive.css" rel="stylesheet">
  <script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.2.23/angular.min.js"></script>
  <script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.6.4/angular.min.js"></script>
  <script src="js/bookorder.js"></script>
</head>
<style>
  body    { padding-top:50px; background-color: lavender;}
</style>
<body class="container" ng-app="myApp" ng-controller="MainCtrl">

<header>
  <nav class="navbar navbar-default">
    <div class="container-fluid" style="background-color:green;">
      <div class="navbar-header" >
		<ul class="nav navbar-nav">
        <li><img src="images/gallery/library5.jpg" alt="Apparels"  width="300%" height="50%"></li>
	  </ul>
      </div>
    </div>
  </nav>
</header>

<main>

	<div class="container">

<div class="row">
                        <form  method="post" action="/adminhome2">
                            <button type="submit" class="btn btn-default" >Home</button>
                        </form>
</div>
<div class="row">

<div class="row">
  <form  method="post" action="/billing" ng-submit="store()">

<div class="col-sm-2">
<button type="button" class="btn btn-default" ng-click="getDataBookOrder()" >Get Book Order Details</button>



<option selected> Select Book Code </option>

<select class="brower-default custom-select" ng-model="bookcode" ng-change="add()">
<option ng-repeat="book in books" value="{{book.billno}}">{{book.billno}} </option>

</select>

</div>

<div class="col-md-8" align="center">
<table class="table table-striped">
<thead>

<tr>
<th>ISBN</th>
<th>Title</th>
<th>Author</th>
<th>Price</th>
<th>Quantity</th>
<th>Discount</th>
<th>Total</th>
<th>Del Name</th>
<th>Delivery Address</th>
<th>Del Phone</th>

</tr>
</thead>
<tbody>
<tr mdbTableCol ng-repeat="book in addBooks | filter:bookcode" >
<td>{{book.isbn}}</td>
<td>{{book.title}}</td>
<td>{{book.author}}</td>
<td>{{book.price}}</td>
<td>{{book.quantity}}</td>
<td>{{book.discount}}</td>
<td>{{book.total}}</td>
<td>{{book.delname}}</td>
<td>{{book.deladd}}</td>
<td>{{book.delphone}}</td>

</tr>
</tbody>
</table>

</div>
	</form>
      </div>


</main>
</body>
</html>
